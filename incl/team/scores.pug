	];

.box-container
	.box-title.-bottom-border
		.title TOP SCORERS
	.box-inner
		include division-dropdown
		.box-content
			.top-scorers-wrapper
				.top-scorers-head
					.th Matches
					.th Goals
				each item in topScores
					.top-predictors-item
						a.top-predictors-link(href=item.src)
							img.ic(src=item.ic)
						.top-scorers-detail
							.name= item.name
							.top-scorers-outcome
								.count= item.matches
								.count= item.goals
